{
  "name": "@tgsnake/core",
  "version": "1.3.1-beta.1",
  "description": "Pure Telegram MTProto framework for nodejs",
  "main": "./lib/src/index.js",
  "devDependencies": {
    "@types/node": "18.11.18",
    "fast-csv": "4.3.6",
    "prettier": "2.8.1",
    "ts-node": "10.9.1",
    "typedoc": "0.23.23",
    "typescript": "4.9.4"
  },
  "scripts": {
    "dev": "ts-node ./test/index.ts",
    "build": "tsc",
    "build:api": "node ./generator/api",
    "build:license": "node ./generator/license",
    "build:error": "node ./generator/error",
    "build:docs": "NODE_OPTIONS=--max_old_space_size=4096 typedoc",
    "prepare": "yarn build",
    "prettier": "NODE_OPTIONS=--max_old_space_size=4096 prettier -w ."
  },
  "homepage": "https://tgsnake.js.org",
  "keywords": [
    "tgsnake",
    "mtproto",
    "telegram"
  ],
  "bugs": "https://github.com/tgsnake/core/issues",
  "repository": "https://github.com/tgsnake/core",
  "author": "butthx",
  "files": [
    "lib/**/**"
  ],
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0",
    "yarn": ">=1.0.0"
  },
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "@tgsnake/log": "latest",
    "async-mutex": "0.4.0",
    "big-integer": "1.6.51"
  },
  "isBeta": true,
  "isPrivate": true
}
