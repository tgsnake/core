{
  "name": "@tgsnake/core",
  "version": "1.0.2",
  "description": "core framework for tgsnake for connecting to telegram api",
  "main": "./lib/index.js",
  "devDependencies": {
    "@types/node": "17.0.35",
    "fast-csv": "4.3.6",
    "prettier": "2.6.2",
    "ts-node": "10.8.0",
    "typescript": "4.7.3"
  },
  "scripts": {
    "dev": "ts-node ./test/index.ts",
    "build": "tsc",
    "build:api": "node ./generator/api",
    "build:license": "node ./generator/license",
    "build:error": "node ./generator/error",
    "prepare": "yarn build",
    "prettier": "NODE_OPTIONS=--max_old_space_size=4096 prettier -w ."
  },
  "homepage": "https://tgsnake.js.org",
  "keywords": [
    "tgsnake",
    "mtproto",
    "telegram"
  ],
  "bugs": "https://github.com/tgsnake/core/issues",
  "repository": "https://github.com/tgsnake/core",
  "author": "butthx",
  "files": [
    "lib/**/**"
  ],
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "@tgsnake/log": "latest",
    "big-integer": "1.6.51"
  }
}
